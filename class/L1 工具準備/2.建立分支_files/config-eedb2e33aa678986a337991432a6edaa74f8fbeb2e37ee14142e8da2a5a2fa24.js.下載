CKEDITOR.editorConfig=function(e){e.toolbarGroups=[{name:"undo"},{name:"links"},{name:"insert"}],e.extraAllowedContent="span[data-mentioned-id](mentioned_name, atwho-inserted);",e.removeButtons="Underline,Subscript,Superscript,Anchor,Image,Table,HorizontalRule,SpecialChar",e.format_tags="p;h1;h2;h3;pre",e.removePlugins="elementspath",e.removeDialogTabs="image:advanced;link:advanced;link:target;",e.height=150,e.extraPlugins="confighelper"};